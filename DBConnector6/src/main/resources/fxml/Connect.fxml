                              <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                              <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <ComboBox fx:id="historyComboBox">
                                 <GridPane.margin>
                                    <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                                 </GridPane.margin>
                              </ComboBox>
                              <TextField fx:id="libraryPathTextField" GridPane.rowIndex="1">
                                 <GridPane.margin>
                                    <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                                 </GridPane.margin>
                              </TextField>
                              <TextField fx:id="driverTextField" GridPane.rowIndex="2">
                                 <GridPane.margin>
                                    <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                                 </GridPane.margin>
                              </TextField>
                              <TextField fx:id="urlTextField" GridPane.rowIndex="3">
                                 <GridPane.margin>
                                    <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                                 </GridPane.margin>
                              </TextField>
                              <TextField fx:id="userTextField" GridPane.rowIndex="4">
                                 <GridPane.margin>
                                    <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                                 </GridPane.margin>
                              </TextField>
                              <TextField fx:id="passwordTextField" GridPane.rowIndex="5">
                                 <GridPane.margin>
                                    <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                                 </GridPane.margin>
                              </TextField>
                              <Label text="※クラスパスにJDBCドライバを含めた場合は Library path、Driver は不要" GridPane.rowIndex="6">
                                 <GridPane.margin>
                                    <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                                 </GridPane.margin>
                              </Label>
                           </children>
                        </GridPane>
                     </center>
                  </BorderPane>
               </center>
               <bottom>
                  <FlowPane alignment="TOP_RIGHT" columnHalignment="CENTER" BorderPane.alignment="CENTER">
                     <children>
                        <Button fx:id="okButton" mnemonicParsing="false" onAction="#onOk" text="OK">
                           <FlowPane.margin>
                              <Insets left="3.0" right="3.0" />
                           </FlowPane.margin>
                        </Button>
                        <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onCancel" text="Cancel">
                           <FlowPane.margin>
                              <Insets left="3.0" right="3.0" />
                           </FlowPane.margin>
                        </Button>
                        <Button fx:id="testButton" mnemonicParsing="false" onAction="#onTest" text="Test">
                           <FlowPane.margin>
                              <Insets left="3.0" right="3.0" />
                           </FlowPane.margin>
                        </Button>
