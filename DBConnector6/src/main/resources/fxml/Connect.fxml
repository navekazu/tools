                     <padding>
                        <Insets bottom="10.0" top="3.0" />
                     </padding>
                  </FlowPane>
               </bottom>
               <padding>
                  <Insets bottom="3.0" />
               </padding>
            </BorderPane>
         </bottom>
      </BorderPane>
