{

    "manifest_version": 2,
    "name": "Goto Google (Ctrl+Shift+F)",
    "version": "1.0",

    "description": "Goto Google (Ctrl+Shift+F)",

    "icons": {
      "48": "icons/goto-google-48.png"
    },

    "browser_action": {
        "default_icon": {
            "16": "icons/goto-google-16.png",
            "32": "icons/goto-google-32.png"
        }
    },

    "background": {
        "scripts": ["goto-google.js"]
    },

    "commands": {
        "toggle-feature": {
            "suggested_key": {
              "default": "Ctrl+Shift+F",
              "linux": "Ctrl+Shift+F"
             },
             "description": "Goto Google (Ctrl+Shift+F)"
        }
    }

}
